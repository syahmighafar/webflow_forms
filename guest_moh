    <script>
      $(function() {
        $("#scDate").datepicker({
          minDate: 2,
          dateFormat: "mm-dd-yy",
          beforeShowDay: function(date) {
            var day = date.getDay();
            return [(day != 0), ''];
          }
        });

        $("#dob").datepicker({
          dateFormat: "mm-dd-yy",
          changeMonth: true,
          changeYear: true,
          yearRange: '1900:2021'
        });
      });
    </script>

    <!-- Input Mask JavaScript -->


    <script>
      let inputElement1 = document.getElementById('bruhims');
      let inputElement2 = document.getElementById('icNumber');

      let inputElement3 = document.getElementById('address_1');
      let inputElement4 = document.getElementById('address_2');
      let inputElement5 = document.getElementById('Simpang');

      var patternMask = IMask(inputElement1, {
        mask: '{BN}00000000',
      });

      var patternMask = IMask(inputElement2, {
        mask: '00000000',
      });

      var patternMask = IMask(inputElement3, {
        mask: '{Kg }**********************************************************************',
      });

      var patternMask = IMask(inputElement4, {
        mask: '{Jln }**********************************************************************',
      });

      var patternMask = IMask(inputElement5, {
        mask: '{Spg }**********************************************************************',
      });
    </script>



    <script>
      const phoneInputField = document.querySelector("#contact_1");
      const phoneInput = window.intlTelInput(phoneInputField, {
        preferredCountries: ["bn", "id", "my", "sg"],
        utilsScript: "https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/js/utils.js",
      });

      phoneInputField.addEventListener("input", function() {
        validatePhone();
        let code = phoneInput.getSelectedCountryData().dialCode;
        document.getElementById("code").setAttribute("value", code);
      });

      function validatePhone() {
        if (!(phoneInput.isValidNumber())) {
          phoneInputField.setCustomValidity("Invalid Phone Number");
        } else {
          phoneInputField.setCustomValidity('');
        }
      }


      const phoneInputField_2 = document.querySelector("#contact_2");
      const phoneInput_2 = window.intlTelInput(phoneInputField_2, {
        preferredCountries: ["bn", "id", "my", "sg"],
        utilsScript: "https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/js/utils.js",
      });

      phoneInputField_2.addEventListener("input", function() {
        validatePhone();
        let code_2 = phoneInput_2.getSelectedCountryData().dialCode;
        document.getElementById("code_2").setAttribute("value", code_2);
      });

      function validatePhone_2() {
        if (!(phoneInput_2.isValidNumber())) {
          phoneInputField_2.setCustomValidity("Invalid Phone Number");
        } else {
          phoneInputField_2.setCustomValidity('');
        }
      }

      const phoneInputField_3 = document.querySelector("#Requester-Contact-Number");
      const phoneInput_3 = window.intlTelInput(phoneInputField_3, {
        preferredCountries: ["bn", "id", "my", "sg"],
        utilsScript: "https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/js/utils.js",
      });

      phoneInputField_3.addEventListener("input", function() {
        validatePhone();
        let code_3 = phoneInput_3.getSelectedCountryData().dialCode;
        document.getElementById("code_3").setAttribute("value", code_3);
      });

      function validatePhone_3() {
        if (!(phoneInput_3.isValidNumber())) {
          phoneInputField_3.setCustomValidity("Invalid Phone Number");
        } else {
          phoneInputField_3.setCustomValidity('');
        }
      }
    </script>

